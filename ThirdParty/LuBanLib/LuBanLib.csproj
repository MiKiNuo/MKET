      </Compile>
      <Compile Include="..\..\Unity\Assets\ThirdParty\LubanLib\SimpleJSON\SimpleJSONBinary.cs">
        <Link>SimpleJSONBinary.cs</Link>
      </Compile>
      <Compile Include="..\..\Unity\Assets\ThirdParty\LubanLib\SimpleJSON\SimpleJSONDotNetTypes.cs">
        <Link>SimpleJSONDotNetTypes.cs</Link>
      </Compile>
      <Compile Include="..\..\Unity\Assets\ThirdParty\LubanLib\SimpleJSON\SimpleJSONUnity.cs">
